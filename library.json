{
    "name": "htcw_uix",
    "version": "0.3.1",
    "description": "A UI library for embedded and IoT devices, for use with htcw_gfx",
    "keywords": ["tft", "UX","UI","GUI","user-experience","user-interface", "graphics", "spi", "parallel", "i2c", "drawing", "jpg", "ttf", "font"],
    "authors": {
        "name":"honey the codewitch",
        "homepage": "https://www.codeproject.com/Members/code-witch"
    },
    "license": "MIT",
    "frameworks": "*",
    "platforms": "*",
    "build": {
        "unflags": "-std=gnu++11",
        "flags": "-std=gnu++14"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/codewitch-honey-crisis/uix.git"
    },
    "dependencies": {
        "codewitch-honey-crisis/htcw_gfx": "^1.634"
    },
    "examples": [
        {
            "name": "TTGO Basic",
            "base": "examples/ttgo_basic",
            "files": [
                "platformio.ini",
                "include/assets/bee_icon.hpp",
                "include/assets/telegrama.hpp",
                "src/main.cpp"
            ]
        },
        {
            "name": "Wio Terminal Basic",
            "base": "examples/wio_terminal_basic",
            "files": [
                "platformio.ini",
                "include/assets/bee_icon.hpp",
                "include/assets/telegrama.hpp",
                "src/main.cpp"
            ]
        },
        {
            "name": "Lilygo T5 4.7in Basic",
            "base": "examples/lilygo_t5_4_7_basic",
            "files": [
                "platformio.ini",
                "include/assets/bee_icon.hpp",
                "include/assets/telegrama.hpp",
                "src/main.cpp"
            ]
        }
    ],
    "export": {
        "exclude":
        [
            ".vscode/*.*",
            "lib/*.*"
        ]
    }
}